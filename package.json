{
  "name": "docker-prisma-studio",
  "version": "1.0.1",
  "description": "SQL Database Web UI for the 21st century.",
  "repository": "https://github.com/timothymiller/docker-prisma-studio.git",
  "author": "tim.miller@preparesoftware.com",
  "license": "GPLv3",
  "private": true,
  "scripts": {
    "build": "docker build -t timothyjmiller/prisma-studio:latest .",
    "dockerhub": "docker push timothyjmiller/prisma-studio:latest",
    "build:all": "docker buildx build --platform linux/ppc64le,linux/s390x,linux/arm/v6,linux/arm/v7,linux/arm64/v8,linux/amd64 --tag timothyjmiller/prisma-studio:latest .",
    "dockerhub:all": "docker buildx build --platform linux/ppc64le,linux/s390x,linux/arm/v6,linux/arm/v7,linux/arm64/v8,linux/amd64 --tag timothyjmiller/prisma-studio:latest --push ."
  },
  "dependencies": {
    "@prisma/cli": "^2.14.0"
  },
  "devDependencies": {
  }
}
